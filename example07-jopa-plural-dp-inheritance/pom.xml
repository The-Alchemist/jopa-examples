    </description>

    <properties>
        <org.springframework.boot.version>1.4.1.RELEASE</org.springframework.boot.version>
        <com.fasterxml.jackson.version>2.4.4</com.fasterxml.jackson.version>
        <ch.qos.logback.version>1.1.2</ch.qos.logback.version>
        <cz.cvut.kbss.jaxb-jsonld.version>0.0.6</cz.cvut.kbss.jaxb-jsonld.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>cz.cvut.kbss.jopa</groupId>
            <artifactId>ontodriver-sesame</artifactId>
            <version>${jopa.version}</version>
        </dependency>

        <dependency>
            <groupId>cz.cvut.kbss</groupId>
            <artifactId>jaxb-jsonld-jackson</artifactId>
            <version>${cz.cvut.kbss.jaxb-jsonld.version}</version>
        </dependency>

        <!-- Spring Boot -->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter</artifactId>
            <version>${org.springframework.boot.version}</version>
